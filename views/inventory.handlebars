<table class="table table-hover">
  <thead>
    <tr>
      <th scope="col">#ID</th>
      <th scope="col">Product Name</th>
      <th scope="col">Current Quanity</th>
      <th scope="col">Quanity Needed For The Week</th>
    </tr>
  </thead>
  <tbody>
    {{#each item}}
    <tr>
      <th scope="row">{{inventory.id}}</th>
      <td>{{inventory.productName}}</td>
      <td>{{inventory.currentQuanity}}</td>
      <td>{{inventory.weeklyQuanity}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>